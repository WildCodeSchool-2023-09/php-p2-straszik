<!doctype html>
<html lang="fr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="/assets/images/favicon.png">
    <title>{% block title %}{% endblock %}</title>

    <link rel="stylesheet" href="/assets/css/style.css">
    {% block stylesheet %}{% endblock %}
</head>

<body>
    {% include 'Include/_navbar.html.twig'%}
    
    <main class="uk-container-fluid uk-margin-medium-top home" uk-grid>
        {% include 'Include/_aside.html.twig' %}
        {% block content %}{% endblock %}
    </main>
    {% include 'Include/_footer.html.twig'%}
    {% block javascript %}
        <script src="https://cdn.jsdelivr.net/npm/uikit@3.17.5/dist/js/uikit.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/uikit@3.17.8/dist/js/uikit-icons.min.js"></script>
    {% endblock %}
    
</body>
</html>
